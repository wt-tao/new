.box1{
  width: 100rpx;
  height: 100rpx;
  background: #f00;
  animation: show 3s;
}
@keyframes show {
  0% {
    opacity: 0;
  }

  25% {
    opacity: 0.25;
  }

  50% {
    opacity: 0.5;
  }

  75% {
    opacity: 0.75;
  }

  100% {
    opacity: 1;
  }
}